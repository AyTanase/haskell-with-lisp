(defmodule Fact)

(define (fact0 0) 1)
(define (fact0 n) (* n (fact0 (1- n))))

(define fact1
  (labels ((fact' (p 0) p)
           (fact' (p n)
             (fact' (* p n) (1- n))))
    (fact' 1)))

(defun fact2 (n)
  (product (enum-from 1 :to n)))

(defun fact3 (n)
  (product (take n (enum-from 1))))
